{
    "name": "uafrica/commonmark-ext",
    "type": "commonmark-extension",
    "description": "Extended parsers and renderers for The PHP League CommonMark parser",
    "keywords": ["markdown", "strikethrough", "strikeout", "commonmark"],
    "require": {
        "league/commonmark": "^0.18"
    },
    "require-dev": {
        "phpunit/phpunit": "5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Meiring",
            "email": "johan@uafrica.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CommonMarkExt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonMarkExt\\Tests\\": "tests/"
        }
    }
}
